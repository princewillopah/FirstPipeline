pipeline {
	agent any     
	stages{
		stage ('one'){
			
			steps{
				echo "executing version VERSION"
			}
		}
		stage ('Two'){
			steps{
				input("hi, do you wanna proceed?")
			}
			
		}
		
		stage ('Three'){
		
			when{
				not {
					branch "master"
				}
			}
			steps {
				echo "Hello"
			}
			
		}
		stage("Four"){
			parallel {
				stage("Unit Test"){
					steps{
						echo "Running the unit test"
					}
				}
				stage("Integration Test"){
					// agent {
					// 	docker {
					// 		reuseNode false
					// 		image 'ubuntu'
					// 	}
					// }
					steps{
						echo "Running the integration test .."
					}
					
				}
			}
		}
	}

}